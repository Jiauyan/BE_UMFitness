{
    "version": 2,
    "functions": {
      "api/index.js": {
        "runtime": "nodejs14.x",
        "memory": 3008
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ]
  }
  